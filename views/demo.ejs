    <div class="container">
      <div id="hc-mask" class="hc-mask"></div>
      <div id="hc-chart-mask" class="hc-mask"></div>

      <!-- Main component for a primary marketing message or call to action -->
      <div class="row hc-chart-row">
        <div id="chart" class="col-md-12 hc-chart"></div>
      </div>

      <div class="row">
        <div id="neighborInfo" class="col-md-12 hc-neighbor-info">
          <!-- <div class="row"> -->
            <div class="col-sm-6 col-md-4 input-group hc-neighbor-num-group">
              <span class="input-group-addon">Number of neighbors to list:</span>
              <select name="neighborNum" id="neighborNum" class="form-control hc-neighbor-num" placeholder="Number of neighbors to list"></select>
            </div>
          <!-- </div> -->
          
          <div class="panel panel-success">
            <div class="panel-heading">
              Information of Neighbors 
            </div>
            
            <div class="table-responsive">
              <table class="table">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Similarity</th>
                    <th></th>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Genre</th>
                  </tr>
                </thead>
                <tbody>
                  
                </tbody>
              </table>
            </div>
          </div>
        
        </div>
      </div>

      <div class="row">
        <div class="col-md-4">
          <div id="hc-sidebar" class="hc-sidebar">
            <div id="selectedVideo" class="selectedVideo">
              <h4>Hits In First 7 Days</h4>
              <h1>...</h1>
              <h4>Hits In Next 30 Days</h4>
              <h1>...</h1>
              <h4>Future / Past</h4>
              <h1>...</h1>
              <h4>Video Information</h4>
              <div id="thumbnailSelectedVideo" class="thumbnail hc-thumbnail hc-thumbnail-active">
                <div class="caption">
                  <h4>Loading...</h4>
                  <dl>
                    <dt>ID</dt>
                    <dd>...</dd>
                    <dt>Genre</dt>
                    <dd>...</dd>
                  </dl>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /.col-md-4 -->

        <div class="col-md-8">
          <div id="listBlock" class="hc-list-block">
            <div class="input-group">
              <input type="text" id="queryDate" class="form-control form_datetime" readonly>
              <span class="input-group-btn">
                <button class="btn btn-primary" id="changeQueryDate" data-loading-text="loading...">Search</button>
              </span>
            </div>
            <span class="help-block">Select the query date. The new videos on that date will be listed.</span>
            

            <div id="pageNav" class="row pageNav">
              <div class="col-sm-6">
                <ul class="pager">
                  <li><a class="hc-previous">&larr; Older</a></li>
                  <li><a class="hc-next">Newer &rarr;</a></li>
                </ul>
              </div>
              <div class="col-sm-6">
                <div class="input-group">
                  <span class="input-group-addon">Page</span>
                  <input id="currentPage" type="text" class="form-control">
                  <span id="totalPage" class="input-group-addon">/20</span>
                  <span class="input-group-btn"><button id="jumpToPage" class="btn btn-primary">Go!</button></span>
                </div>
              </div>
            </div>
            <div id="progList" class="row hc-prog-list">
              
            </div>
            
            <div class="row visible-xs visible-sm">
              <div class="col-md-12">
                <ul class="pager">
                  <li><a class="hc-previous">&larr; Older</a></li>
                  <li><a class="hc-next">Newer &rarr;</a></li>
                </ul>
              </div>
            </div>
          </div>
        </div> <!-- /.col-md-9 -->
      </div> <!-- /.row -->

    </div> <!-- /container -->

